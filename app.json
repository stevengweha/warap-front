{
  "expo": {
    "name": "WA-RAP",
    "slug": "WA-RAP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "warap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "display": "standalone",
      "favicon": "./assets/images/favicon.png",
      "themeColor": "#4CAF50",
      "backgroundColor": "#ffffff",
      "serviceWorker": {
        "register": true
      },
      "manifest": {
        "short_name": "Warap",
        "name": "WA-RAP",
        "start_url": ".",
        "background_color": "#ffffff",
        "theme_color": "#4CAF50",
        "orientation": "portrait",
        "display": "standalone",
        "icons": [
          {
            "src": "./assets/images/icon.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/images/icon.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f288d854-c791-408c-8c90-21df852de5e7"
      }
    }
  }
}
